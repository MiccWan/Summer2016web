<% include ../partials/header.ejs %>
<h1>Username - <%= currentUser.username %></h1>
<% var sum1 = 0, sum2 = 0 %>
<% currentUser.rank.python.forEach(function(p) { %>
	<% if (p) { %>
		<% sum1 += p %>	
	<% } %>
<% }) %>
<% currentUser.rank.cpp.forEach(function(p) { %>
	<% if (p) { %>
		<% sum2 += p %>	
	<% } %>
	
<% }) %>
<% var sum = sum1 + sum2  %>
<p>Score - <%= sum %></p>
<ul>
<li>Python</li>
<% for (var i = 0; i < pyCnt; i++) { %>
	<% if (currentUser.judges.python[i]) { %>
		<li><%= i + 1 %> - <%= currentUser.judges.python[i] %></li>
	<% } else {%>
		<li><%= i + 1 %> - N/A</li>
	<% } %>
<% } %>
</ul>
<ul>
<li>C++</li>
<% for (var i = 0; i < cppCnt; i++) { %>
	<% if (currentUser.judges.cpp[i]) { %>
		<li><%= i + 1 %> - <%= currentUser.judges.cpp[i] %></li>
	<% } else {%>
		<li><%= i + 1 %> - N/A</li>
	<% } %>
<% } %>
</ul>
