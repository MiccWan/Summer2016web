<% var rank = 1 %>
<% var index = 1 %>
<% var prev = 0 %>
<div class='bg'><div class='inner'>
<table>
	<tr>
		<th>Rank</th>
		<th>Username</th>
		<th>Nickname</th>
		<th>Score</th>
	</tr>
	<% for (var i = 0; i < allUser.length; i++) { %>
		<tr>
			<% if (rankSum(allUser[i]) == prev) { %>
				<td><%=rank%></td>
				<td><a href="/profile/<%=allUser[i].username%>"><%=allUser[i].username%></a></td>
				<td><a href="/profile/<%=allUser[i].username%>"><%=allUser[i].nickname%></a></td>
				<td><%=rankSum(allUser[i])%></td>
				<% index++ %>
			<% } else { %>
			<%rank = index %>
			<%prev = rankSum(allUser[i]) %>
				<td><%=rank%></td>
				<td><a href="/profile/<%=allUser[i].username%>"><%=allUser[i].username%></a></td>
				<td><a href="/profile/<%=allUser[i].username%>"><%=allUser[i].nickname%></a></td>
				<td><%=rankSum(allUser[i])%></td>
				<% index++%>
			<% } %>
		</tr>
	<% } %>
</table>
